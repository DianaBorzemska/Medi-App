<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Medi_Predict">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="409dp"
        android:layout_height="729dp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp">

        <TextView
            android:id="@+id/heartDiseasePredictionAccuracy"
            android:layout_width="116dp"
            android:layout_height="49dp"
            android:layout_marginBottom="20dp"
            android:autoText="false"
            android:elegantTextHeight="true"
            android:ems="10"
            android:inputType="number"
            android:text="86.89%"
            android:textAlignment="center"
            app:layout_constraintBottom_toTopOf="@+id/alzheimersPredictionAccuracy"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.492"
            app:layout_constraintStart_toEndOf="@+id/textView6" />

        <TextView
            android:id="@+id/textView33"
            android:layout_width="206dp"
            android:layout_height="49dp"
            android:layout_marginBottom="20dp"
            android:elegantTextHeight="true"
            android:text="Heart Disease prediction accuracy"
            app:layout_constraintBottom_toTopOf="@+id/textView41"
            tools:layout_editor_absoluteX="16dp" />

        <TextView
            android:id="@+id/textView41"
            android:layout_width="206dp"
            android:layout_height="49dp"
            android:layout_marginBottom="188dp"
            android:elegantTextHeight="true"
            android:text="Alzheimer's prediction accuracy"
            app:layout_constraintBottom_toBottomOf="parent"
            tools:layout_editor_absoluteX="16dp" />

        <TextView
            android:id="@+id/diabetesPredictionAccuracy"
            android:layout_width="116dp"
            android:layout_height="49dp"
            android:layout_marginBottom="20dp"
            android:elegantTextHeight="true"
            android:ems="10"
            android:inputType="number"
            android:text="82,25%"
            android:textAlignment="center"
            app:layout_constraintBottom_toTopOf="@+id/heartDiseasePredictionAccuracy"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.492"
            app:layout_constraintStart_toEndOf="@+id/textView6" />

        <TextView
            android:id="@+id/alzheimersPredictionAccuracy"
            android:layout_width="116dp"
            android:layout_height="49dp"
            android:elegantTextHeight="true"
            android:ems="10"
            android:inputType="number"
            android:text="89.33%"
            android:textAlignment="center"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.492"
            app:layout_constraintStart_toEndOf="@+id/textView6"
            tools:layout_editor_absoluteY="492dp" />

        <Button
            android:id="@+id/predictButton"
            android:layout_width="202dp"
            android:layout_height="72dp"
            android:text="PREDICT"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.891" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="303dp"
            android:layout_height="81dp"
            android:layout_marginTop="16dp"
            android:text="The risk of you contracting these illnesses"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="206dp"
            android:layout_height="49dp"
            android:layout_marginStart="16dp"
            android:layout_marginBottom="20dp"
            android:elegantTextHeight="true"
            android:text="Diabetes"
            app:layout_constraintBottom_toTopOf="@+id/textView5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="206dp"
            android:layout_height="49dp"
            android:layout_marginStart="16dp"
            android:layout_marginBottom="20dp"
            android:elegantTextHeight="true"
            android:text="Heart Disease"
            app:layout_constraintBottom_toTopOf="@+id/textView6"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="206dp"
            android:layout_height="49dp"
            android:layout_marginStart="16dp"
            android:layout_marginBottom="20dp"
            android:elegantTextHeight="true"
            android:text="Alzheimer's"
            app:layout_constraintBottom_toTopOf="@+id/textView32"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/diabetesPrediction"
            android:layout_width="116dp"
            android:layout_height="49dp"
            android:layout_marginTop="144dp"
            android:layout_marginBottom="20dp"
            android:ems="10"
            android:inputType="number"
            app:layout_constraintBottom_toTopOf="@+id/editTextNumber2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.492"
            app:layout_constraintStart_toEndOf="@+id/textView4"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/heartDIseasePrediction"
            android:layout_width="116dp"
            android:layout_height="49dp"
            android:layout_marginBottom="20dp"
            android:ems="10"
            android:inputType="number"
            app:layout_constraintBottom_toTopOf="@+id/alzheimersPrediction"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.492"
            app:layout_constraintStart_toEndOf="@+id/textView5" />

        <TextView
            android:id="@+id/alzheimersPrediction"
            android:layout_width="116dp"
            android:layout_height="49dp"
            android:layout_marginBottom="20dp"
            android:ems="10"
            android:inputType="number"
            app:layout_constraintBottom_toTopOf="@+id/diabetesPredictionAccuracy"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.492"
            app:layout_constraintStart_toEndOf="@+id/textView6" />

        <TextView
            android:id="@+id/textView32"
            android:layout_width="206dp"
            android:layout_height="49dp"
            android:layout_marginBottom="20dp"
            android:elegantTextHeight="true"
            android:text="Diabetes prediction accuracy"
            app:layout_constraintBottom_toTopOf="@+id/textView33"
            tools:layout_editor_absoluteX="16dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>